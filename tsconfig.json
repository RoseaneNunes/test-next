{
  "compilerOptions": {
    "target": "ES6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "node"
    ],
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "moduleResolution": "node"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/pages/_app.jsx",
    "src/components/CountDown.tsx"
, "src/components/perfil.jsx", "src/components/ChallengerBox.jsx", "src/components/CompletedChanllenges.jsx", "src/components/ExperienceBar.jsx", "src/context/ChallengesContext.tsx"  ],
  "exclude": [
    "node_modules"
  ]
}
